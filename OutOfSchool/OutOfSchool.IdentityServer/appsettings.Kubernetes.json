{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Email": {
    "Enabled": false
  },
  "IdentityAccessConfig": {
    "AdditionalIdentityClients": [
      {
        "ClientId": "angular",
        "RedirectUris": [
          "http://localhost"
        ],
        "PostLogoutRedirectUris": [
          "http://localhost"
        ],
        "AllowedCorsOrigins": [
          "http://localhost"
        ]
      },
      {
        "ClientId": "Swagger",
        "RedirectUris": [
          "http://localhost/web/swagger/oauth2-redirect.html"
        ],
        "PostLogoutRedirectUris": [
          "http://localhost/web/swagger/oauth2-redirect.html"
        ],
        "AllowedCorsOrigins": [
          "http://localhost/web/"
        ]
      }
    ]
  },
  "Issuer": {
    "Uri": "http://localhost/identity",
    "CertificateExpirationDays": 7
  }
}