{
  "Serilog": {
    "Using": [ "Serilog.Sinks.GoogleCloudLogging" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "GoogleCloudLogging",
        "Args": {
          "useJsonOutput": "true",
          "useLogCorrelation": "true"
        }
      }
    ]
  },
  "Identity": {
    "Authority": "https://auth.oos.dmytrominochkin.cloud"
  },
  "IdentityAccessConfig": {
    "AdditionalIdentityClients": [
      {
        "ClientId": "angular",
        "RedirectUris": [
          "http://localhost:4200",
          "https://localhost:4200",
          "https://oos.dmytrominochkin.cloud"
        ],
        "PostLogoutRedirectUris": [
          "http://localhost:4200",
          "https://localhost:4200",
          "https://oos.dmytrominochkin.cloud"
        ],
        "AllowedCorsOrigins": [
          "http://localhost:4200",
          "https://localhost:4200",
          "https://oos.dmytrominochkin.cloud"
        ]
      },
      {
        "ClientId": "Swagger",
        "RedirectUris": [
          "https://api.oos.dmytrominochkin.cloud/swagger/oauth2-redirect.html"
        ],
        "PostLogoutRedirectUris": [
          "https://api.oos.dmytrominochkin.cloud/swagger/oauth2-redirect.html"
        ],
        "AllowedCorsOrigins": [
          "https://api.oos.dmytrominochkin.cloud"
        ]
      }
    ]
  },
  "Issuer": {
    "Uri": "https://auth.oos.dmytrominochkin.cloud",
    "CertificateExpirationDays": 7
  }
}
